#include<unordered_map>
class Solution {
public:
    string kthDistinct(vector<string>& arr, int k) {
        unordered_map<string,int>ans;
        int i=0;
        int m=0;
        int x=arr.size();
        while(i<x)
        {
            string s=arr[i];
            ans[s]++;
            i++; 
        }
        for(auto n=0;n<x;n++)
        {
            string s=arr[n];
            if(ans[s]==1)
            {
                m++;
                if(m==k)return s;
            }
        }
        return "";
    }
};
